{
    "name": "wisembly/clearbit-php",
    "description": "Clearbit API client built on top of Guzzle 3",
    "keywords": ["clearbit", "clearbit.com", "api", "guzzle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Wisembly",
            "homepage": "http://wisembly.com"
        }
    ],

    "require": {
        "php": ">=5.4.0",
        "guzzle/guzzle": "~3.9",
        "symfony/property-access": "^2.3 || ^3.0"
    },

    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },

    "autoload": {
        "psr-4": {
            "Clearbit\\": "src/"
        }
    },

    "config": {
        "bin-dir": "bin"
    }
}
